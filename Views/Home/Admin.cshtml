@using CarInsurance.Models

@model Insuree insuree = new Insuree() 

<div class="row no-gutters m-3">
    <div class="glyphicon-credit-card col-sm-12 col-md-10 col-lg-6 mx-auto shadow">
        <div class="card-header text-center">
            <h5 class="m-0">Quotes</h5>
        </div>
        <div class="card-body">
            {
                <div class="row">
                    <div class="col">
                        @insuree.FirstName
                    </div>
                    <div class="col">
                        @insuree.LastName
                    </div>
                    <div class="col">
                        @insuree.EmailAddress
                    </div>
                    <div class="col">
                        @insuree.Quote
                    </div>
                    }

                </div>
         </div>
    </div>
</div>